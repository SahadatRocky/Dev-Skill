#include<bits/stdc++.h>
using namespace std;
typedef long long int lli;
typedef unsigned long long ull;

int main(){
	   
	   int n;
	   cin>>n;
	   cin.ignore();
	   vector<string>v;
	   for(int i=0;i<n;i++){
	   	  string s;
	   	  getline(cin,s);
	   	  reverse( s.begin(), s.end() );
	   	  v.push_back(s);
	   	  
	   	// cout<<s<<endl;
	   	  
}
	
	vector<string>::iterator  itr;
	
	for(itr=v.begin(); itr!=v.end(); itr++){
		
		cout<< *itr <<endl;
		
		}
	
	
	
  return 0;
}
