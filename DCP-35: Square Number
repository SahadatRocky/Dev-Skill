
#include<bits/stdc++.h>

using namespace std;

int main(){
	
    int n,a,b;
    cin>>n;
   
     for( int i=1; i<=n;i++){
         
         cin>>a;
         
         b=sqrt(a);
         
          if(b*b==a){
    	cout<<"Case "<<i<<": "<<"YES"<<endl;
    
    	}
     else{
     	cout<<"Case "<<i<<": "<<"NO"<<endl;
     	}
         
              	 
    }
  
	  
  	return 0;

}


 
 
