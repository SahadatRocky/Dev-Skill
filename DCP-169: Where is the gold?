#include <bits/stdc++.h>

using namespace std;

int main(){
	
	  char a[100][100];
	  int n,r,c;
	  cin>>n;
	  int k=1;
	  while(n--){
	  	cin>>c>>r;
	  	
	  	     for(int i=1 ; i<=c ; i++){
	  	     	 for(int j=1 ; j<=r ; j++){
	  	     	 	
	  	     	 	cin>>a[i][j];
	  	     	 
	  	     	 	}	
	  	     	}
	  	
	  	
	  	cout<<"Case "<<k<<":"<<endl;
	  	int f=0;
	  	    for(int i=1 ; i<=c ; i++){
	  	     	 for(int j=1 ; j<=r ; j++){
	  	     	 	
	  	     	 	if(a[i][j] == '$'){
	  	     	 		
	  	     	 		cout<<i<<","<<j<<endl;
	  	     	 		}
	  	     	 	else{
	  	     	 		  f++;
	  	     	 		}
	  	     	 
	  	     	 	}	
	  	     	}
	  	
	  	if(f==(c*r)){
	  		cout<<"No Gold Found"<<endl;
	  		}
	  	    
	  	k++;
	  	
	  	}
	
	return 0;
	}
